<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Parallel" thread-count="1">


	<test name="TestOnAndroid">
		<parameter name="config" value="bs.cbt.testSuite.conf.json" />
		<parameter name="environment" value="android" />
		<packages>
			<package name="testSuite"></package>
		</packages>
	</test>

	<test name="TestOnWin10Chrome">
		<parameter name="config" value="bs.cbt.testSuite.conf.json" />
		<parameter name="environment" value="win10chrome" />
		<packages>
			<package name="testSuite"></package>
		</packages>
	</test>



</suite>

